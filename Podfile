# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

use_frameworks!

def data_pod
  pod 'Alamofire'
  pod 'ObjectMapper'
end

target 'Home' do
  pod 'SDWebImage'
end

target 'Favorite' do
  pod 'SDWebImage'
end

target 'Detail' do
  pod 'SDWebImage'
end

target 'Data' do
  data_pod
end
