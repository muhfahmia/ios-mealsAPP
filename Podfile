# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

use_frameworks!

def data_pod
  pod 'Alamofire'
  pod 'ObjectMapper'
end

def ui_pod
  pod 'SDWebImage'
end

target 'Domain' do
  data_pod
end

target 'Data' do
  data_pod
end

target 'Core' do
  ui_pod
end

target 'Home' do
  ui_pod
end

target 'Favorite' do
  ui_pod
end

target 'Detail' do
  ui_pod
end

target 'OnBoarding' do
  ui_pod
end

target 'About' do
  ui_pod
end
